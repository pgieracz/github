/*Program kt√≥ry podaje wynik: dodawania, odejmowania, mnozenia i dzielenia dwoch podanych liczb*/
#include <stdio.h>
main()
{
  int licz1, licz2, wynik;
  printf("Podaj liczby:");
    scanf("%d, %d", &licz1, &licz2);
  wynik=licz1+licz2;
  printf("\n%d + %d = %d", licz1, licz2, wynik);
  wynik=licz1-licz2;
  printf("\n%d - %d = %d", licz1, licz2, wynik);
  wynik=licz1*licz2;
  printf("\n%d*%d=%d", licz1, licz2, wynik);
  wynik=licz1/licz2;
  printf("\n%d:%d=%d\n",licz1, licz2, wynik);

  return 0;
}
